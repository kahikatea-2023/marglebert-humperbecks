{
  "name": "marglebert-humperbecks",
  "module": "src/index.tsx",
  "type": "module",
  "scripts": {
    "start": "bun run src/index.tsx",
    "dev": "concurrently \"bun run dev:server\" \"bun run dev:css\"",
    "dev:server": "bun run --watch src/index.tsx",
    "dev:css": "tailwindcss -i src/input.css -o src/styles/output.css --watch"
  },
  "devDependencies": {
    "bun-types": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/html": "^0.5.2",
    "concurrently": "^8.2.0",
    "elysia": "^0.5.22",
    "tailwindcss": "^3.3.3",
    "typed-html": "^3.0.1"
  }
}
